def PowerA234(A, B, C, D):
   B = A**2
   C = A**3
   D = A**4
   return B, C, D


numbers = [2, 3, 4, 5, 6]  # Пять данных чисел
for num in numbers:
   second_power, third_power, fourth_power = PowerA234(num, 0, 0, 0)
   print(f"Число: {num}, Вторая степень: {second_power}, Третья степень: {third_power}, Четвертая степень: {fourth_power}")

